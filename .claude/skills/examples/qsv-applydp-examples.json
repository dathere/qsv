    {
      "name": "applydp_ops_regex_replace",
      "description": "Applydp Ops Regex Replace",
      "input": {
        "data": [
          [
            "description"
          ],
          [
            "My SSN is 078-05-1120. Please do not share it."
          ],
          [
            "twinkle",
            "twinkle brownie star",
            "how I wonder what you are"
          ],
          [
            "Somebody from Nigeria called asked for my ssn - 987-65-4320."
          ],
          [
            "Won't fall for that scam!"
          ],
          [
            "Just enter 987-65-4329 when prompted. Also try 987-65-1234 if it doesn't work."
          ]
        ],
        "filename": "data.csv"
      },
      "command": "qsv applydp operations regex_replace description data.csv --comparand \"(?:\\\\\\\\d{3}-\\\\\\\\d{2}-\\\\\\\\d{4})\" --replacement SSN",
      "args": [
        "operations",
        "regex_replace",
        "description",
        "data.csv"
      ],
      "options": {
        "--comparand": "(?:\\\\d{3}-\\\\d{2}-\\\\d{4})",
        "--replacement": "SSN"
      },
      "expected": {
        "data": [
          [
            "description"
          ],
          [
            "My SSN is SSN. Please do not share it."
          ],
          [
            "twinkle",
            "twinkle brownie star",
            "how I wonder what you are"
          ],
          [
            "Somebody from Nigeria called asked for my ssn - SSN."
          ],
          [
            "Won't fall for that scam!"
          ],
          [
            "Just enter SSN when prompted. Also try SSN if it doesn't work."
          ]
        ]
      }
    },
