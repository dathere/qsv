    {
      "name": "count_width_no_delims_human_readable",
      "description": "Count Width No Delims Human Readable",
      "command": "qsv count ",
      "options": {
        "-H": "in.csv"
      }
    },
    {
      "name": "count_width_human_readable",
      "description": "Count Width Human Readable",
      "command": "qsv count ",
      "options": {
        "-H": "in.csv"
      }
    },
    {
      "name": "count_width_custom_delimiter",
      "description": "Count Width Custom Delimiter",
      "command": "qsv count ",
      "options": {
        "--width": "in.csv"
      }
    },
    {
      "name": "count_flexible",
      "description": "Count Flexible",
      "command": "qsv count ",
      "options": {
        "--flexible": "in.csv"
      }
    },
    {
      "name": "count_comments",
      "description": "Count Comments",
      "input": {
        "data": [
          [
            "# this is a comment",
            ""
          ],
          [
            "# next comment",
            ""
          ],
          [
            "letter",
            "number"
          ],
          [
            "alpha",
            "13"
          ],
          [
            "beta",
            "24"
          ],
          [
            "# comment here too!",
            "24"
          ]
        ],
        "filename": "in.csv"
      },
      "command": "qsv count in.csv",
      "args": [
        "in.csv"
      ]
    },
    {
      "name": "count_custom_delimiter",
      "description": "Count Custom Delimiter",
      "command": "qsv count in.csv",
      "args": [
        "in.csv"
      ],
      "options": {
        "--delimiter": ";"
      },
      "tags": [
        "custom-delimiter"
      ]
    }
  ]
}