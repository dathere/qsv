{
  "skill": "qsv-count",
  "version": "12.0.0",
  "examples": [
    {
      "name": "count_simple",
      "description": "Count Simple",
      "command": "qsv count in.csv",
      "args": [
        "in.csv"
      ]
    },
    {
      "name": "count_empty",
