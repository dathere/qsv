{
  "skill": "qsv-edit",
  "version": "12.0.0",
  "examples": [
    {
      "name": "edit_by_col_name",
      "description": "Edit By Col Name",
      "input": {
        "data": [
          [
            "letter",
            "number"
          ],
          [
            "a",
            "1"
          ],
          [
            "b",
            "2"
          ]
        ],
        "filename": "data.csv"
      },
      "command": "qsv edit data.csv number 0 3",
      "args": [
        "data.csv",
        "number",
        "0",
        "3"
      ]
    },
    {
      "name": "edit_by_col_index",
      "description": "Edit By Col Index",
      "input": {
        "data": [
          [
            "letter",
            "number"
          ],
          [
            "a",
            "1"
          ],
          [
            "b",
            "2"
          ]
        ],
        "filename": "data.csv"
      },
      "command": "qsv edit data.csv 1 0 3",
      "args": [
        "data.csv",
        "1",
        "0",
        "3"
      ]
    },
    {
      "name": "edit_first_header",
      "description": "Edit First Header",
      "command": "qsv edit data.csv letter 0 character",
      "args": [
        "data.csv",
        "letter",
        "0",
        "character"
      ],
      "options": {
        "--no-headers": "true"
      },
      "tags": [
        "no-headers"
      ]
    },
    {
      "name": "edit_column_index_priority",
      "description": "Edit Column Index Priority",
      "command": "qsv edit data.csv 0 0 z",
      "args": [
        "data.csv",
        "0",
        "0",
        "z"
      ]
    },
    {
      "name": "edit_by_col_underscore",
      "description": "Edit By Col Underscore",
      "input": {
        "data": [
          [
            "letter",
            "number"
          ],
          [
            "a",
            "1"
          ],
          [
            "b",
            "2"
          ]
        ],
        "filename": "data.csv"
      },
      "command": "qsv edit data.csv _ 0 3",
      "args": [
        "data.csv",
        "_",
        "0",
        "3"
      ]
    },
    {
      "name": "edit_in_place",
      "description": "Edit In Place",
      "input": {
        "data": [
          [
            "letter",
            "number"
          ],
          [
            "a",
            "1"
          ],
          [
            "b",
            "2"
          ]
        ],
        "filename": "data.csv"
      },
      "command": "qsv edit data.csv number 0 3",
      "args": [
        "data.csv",
        "number",
        "0",
        "3"
      ],
      "options": {
        "--in-place": "true"
      }
    }
  ]
}