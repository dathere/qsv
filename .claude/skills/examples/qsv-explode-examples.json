{
  "skill": "qsv-explode",
  "version": "12.0.0",
  "examples": [
    {
      "name": "explode",
      "description": "Explode",
      "input": {
        "data": [
          [
            "name",
            "colors"
          ],
          [
            "Mary",
            "yellow"
          ],
          [
            "John",
            "blue|orange"
          ],
          [
            "Jack",
            ""
          ]
        ],
        "filename": "data.csv"
      },
      "command": "qsv explode colors \"|\" data.csv",
      "args": [
        "colors",
        "|",
        "data.csv"
      ],
      "expected": {
        "data": [
          [
            "name",
            "colors"
          ],
          [
            "Mary",
            "yellow"
          ],
          [
            "John",
            "blue"
          ],
          [
            "John",
            "orange"
          ],
          [
            "Jack",
            ""
          ]
        ]
      }
    },
    {
