{
  "skill": "qsv-input",
  "version": "12.0.0",
  "examples": [
    {
      "name": "test_input_comment",
      "description": "Test Input Comment",
      "command": "qsv input --comment \"#\"",
      "options": {
        "--comment": "#"
      },
      "expected": {
        "data": [
          [
            "column1",
            "column2",
            "column3"
          ],
          [
            "a",
            "1",
            "alpha"
          ],
          [
            "b",
            "2",
            "beta"
          ],
          [
            "c",
            "3",
            "gamma"
          ],
          [
            "d",
            "4",
            "delta"
          ],
          [
            "e",
            "5",
            "epsilon"
          ]
        ]
      }
    },
    {
      "name": "test_input_skiplines",
      "description": "Test Input Skiplines",
      "input": {
        "data": [
          [
            "# test file to see how skiplines work",
            ""
          ],
          [
            "! this is another comment before the header",
            ""
          ],
          [
            "# DATA DICTIONARY",
            ""
          ],
          [
            "! column1 - alphabetic; id of the column",
            ""
          ],
          [
            "% column2 - numeric; just a number",
            ""
          ],
          [
            "column1",
            "column2"
          ],
          [
            "a",
            "1"
          ],
          [
            "c",
            "3"
          ],
          [
            "e",
            "5"
          ]
        ],
        "filename": "preamble.csv"
      },
      "command": "qsv input preamble.csv --skip-lines 5",
      "args": [
        "preamble.csv"
      ],
      "options": {
        "--skip-lines": "5"
      },
      "expected": {
        "data": [
          [
            "column1",
            "column2"
          ],
          [
            "a",
            "1"
          ],
          [
            "c",
            "3"
          ],
          [
            "e",
            "5"
          ]
        ]
      }
    },
