{
  "skill": "qsv-jsonl",
  "version": "12.0.0",
  "examples": [
    {
      "name": "jsonl_simple",
      "description": "Jsonl Simple",
      "command": "qsv jsonl data.jsonl",
      "args": [
        "data.jsonl"
      ],
      "expected": {
        "data": [
          [
            "id",
            "father",
            "mother",
            "oldest_child",
            "boy"
          ],
          [
            "1",
            "Mark",
            "Charlotte",
            "Tom",
            "true"
          ],
          [
            "2",
            "John",
            "Ann",
            "Jessika",
            "false"
          ],
          [
            "3",
            "Bob",
            "Monika",
            "Jerry",
            "true"
          ]
        ]
      }
    },
    {
      "name": "jsonl_simple_delimiter",
      "description": "Jsonl Simple Delimiter",
      "command": "qsv jsonl data.jsonl",
      "args": [
        "data.jsonl"
      ],
      "expected": {
        "data": [
          [
            "id;father;mother;oldest_child;boy"
          ],
          [
            "1;Mark;Charlotte;Tom;true"
          ],
          [
            "2;John;Ann;Jessika;false"
          ],
          [
            "3;Bob;Monika;Jerry;true"
          ]
        ]
      },
      "tags": [
        "custom-delimiter"
      ]
    },
