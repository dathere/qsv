{
  "name": "qsv-behead",
  "version": "14.0.0",
  "description": "Drop headers from a CSV.",
  "category": "utility",
  "command": {
    "binary": "qsv",
    "subcommand": "behead",
    "args": [
      {
        "name": "input",
        "type": "file",
        "required": true,
        "description": ""
      }
    ],
    "options": [
      {
        "flag": "--flexible",
        "type": "flag",
        "description": "Do not validate if the CSV has different number of fields per record, increasing performance."
      },
      {
        "flag": "--output",
        "type": "string",
        "description": "Write output to <file> instead of stdout."
      }
    ]
  },
  "hints": {
    "streamable": true,
    "memory": "constant"
  }
}